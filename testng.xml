<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Examples">

    <test name="Parameters">
        <parameter name="int1" value="10"></parameter>
        <parameter name="int2" value="9"></parameter>
        <classes>
            <class name="testng.dataprovider.ParametersExample"></class>
        </classes>
    </test>
    <test name="Class Execution">
        <classes>
            <class name="testng.Demo"></class>
        </classes>
    </test>
    <test name="group Example">
        <groups>
            <run>
                <include name="smoke"></include>
            </run>
        </groups>
        <packages>
            <package name="testng.attributestest"></package>
        </packages>
    </test>

</suite>